{
  "name": "ts-api",
  "version": "1.0.0",
  "description": "API Desenvolvida Utilizando TS, NODE, ES6, Postgres e Sequelize",
  "main": "server/server.ts",
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/ts-node ./server/server.ts",
    "builded": "NODE_ENV=development node ./build/server.js",
    "unit-test": "NODE_ENV=test mocha --opts build/tests/unit/config/mocha.opts ./build/tests/unit/unit.test.js",
    "integration-test": "NODE_ENV=test mocha --opts build/tests/integration/config/mocha.opts ./build/tests/integration/integration.test.js",
    "test": "NODE_ENV=test echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "API",
    "TypeScript",
    "NODE",
    "ES6",
    "curso"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ricardoub/cursos-raphael-lima-ts-api.git"
  },
  "author": "Ricardo Ulrich Bomfim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ricardoub/cursos-raphael-lima-ts-api/issues"
  },
  "homepage": "https://github.com/ricardoub/cursos-raphael-lima-ts-api#readme",
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "express": "^4.16.1",
    "http": "0.0.0",
    "morgan": "^1.9.0",
    "pg": "^7.3.0",
    "sequelize-cli": "^3.0.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.5",
    "@types/body-parser": "^1.16.5",
    "@types/chai": "^4.0.7",
    "@types/express": "^4.0.37",
    "@types/mocha": "^2.2.44",
    "@types/morgan": "^1.7.33",
    "@types/sequelize": "^4.0.76",
    "@types/supertest": "^2.0.4",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "sequelize": "^4.13.5",
    "supertest": "^3.0.0",
    "testdouble": "^3.2.6",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.1"
  }
}
